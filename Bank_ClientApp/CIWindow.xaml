<Window x:Class="Bank_ClientApp.CIWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bank_ClientApp"
        mc:Ignorable="d"
        Title="GreyPine Client Application" 
        Height="720" 
        Width="1280" 
        Icon="Resources/UI/Logos/GreyPine_Icon_x64.ico">
    <Grid Background="#FFCE8D00" Unloaded="Grid_Unloaded">
        <Image x:Name="bgLogo" Margin="10" Source="Resources/UI/Logos/GreyPine_Logo.png">
            <Image.Effect>
                <BlurEffect Radius="20"/>
            </Image.Effect>
        </Image>

    </Grid>
</Window>
